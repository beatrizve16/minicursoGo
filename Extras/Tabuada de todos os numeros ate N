
package main
import "fmt"

func main() {
    
    var num int
    fmt.Print("Digite um nÃºmero: ") 
    fmt.Scanln(&num) 
    
    for i := 1 ; i <= num; i++ {
        fmt.Printf("Tabuada do %d\n" , i)
        for j := 1; j <= 10; j++{
        
            fmt.Printf("%d * %d = %d\n" , i, j, i*j) 
        }
        fmt.Println("")
    }
}
